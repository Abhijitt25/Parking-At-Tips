package com.demo.test;


import java.util.ArrayList;
import java.util.List;

import com.demo.model.Parking_names;
import com.demo.model.User;

public class TestUsers {
public static void main(String[] args)
{
	User u1 = new User(1,"Rohit","8784974897","rohit45@gmail.com","BMW");
	User u2 = new User(2,"Virat","8746155654","virat18@gmail.com","Kia");
	User u3 = new User(3,"Abhi","9784874897","abhi01@gmail.com","Audi");
	User u4 = new User(4,"Bhavesh","9984974897","bhavesh20@gmail.com","Ford");
	
	Parking_names p1 = new Parking_names(100,"OM car parking","Ekta building, Shivaji Nagar","8974849878","ShivajiNagar");
	Parking_names p2 = new Parking_names(200,"MIT car parking","Kale complex,Kothrud","7988894887","Kothrud");
	
	List<User> ulist1 = new ArrayList<>();
	ulist1.add(u1);
	ulist1.add(u2);
	
	List<User> ulist2 = new ArrayList<>();
	ulist2.add(u3);
	ulist2.add(u4);
	
	p1.setUsers(ulist1);
	p2.setUsers(ulist2);
	
	
	List<Parking_names> plist1 = new ArrayList<>();
	plist1.add(p1);
	
	List<Parking_names> plist2 = new ArrayList<>();
	plist2.add(p2);
	
	u1.setParking_names(plist1);
	u2.setParking_names(plist1);
	
	u3.setParking_names(plist2);
	u4.setParking_names(plist2);
	
	
}
}
